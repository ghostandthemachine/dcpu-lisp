

(define (print color bg-color x y text)
  (MUL y 32)
  (ADD y x)
  (ADD y 0x8000)
  (SHL color 12)
  (SHL bg-color 8)
  (BOR text color)
  (BOR text bg-color)
  (SET [y] text))

(define x 1)
(define (o y)
  (if (< y 4)
      (begin
        (print 0xE 1 x y 0x35)
        (o (+ y 1)))))
(o 0)

